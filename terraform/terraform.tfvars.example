# AWS Configuration
aws_region = "ap-southeast-1"

# VPC and Network Configuration
vpc_id    = "vpc-xxxxxxxxx"  # Your VPC ID
subnet_id = "subnet-xxxxxxxxx"  # Your subnet ID (public subnet recommended)

# EC2 Configuration
ami_id   = "ami-0c02fb55956c7d316"  # Amazon Linux 2 AMI (update for your region)
key_name = "your-key-pair-name"     # Your EC2 Key Pair name

# Example values:
# aws_region = "ap-southeast-1"
# vpc_id = "vpc-12345678"
# subnet_id = "subnet-12345678"
# ami_id = "ami-0c02fb55956c7d316"  # Amazon Linux 2 in ap-southeast-1
# key_name = "my-key-pair"